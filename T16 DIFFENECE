n=int(input("enter"))
num=int(input("enter the number"))
max=num
min=max
for i in range (1,n):
    num=int(input())
    if num>max:
        max=num
        a=max
    if num<min:
        min=num
        b=min
print("THE DIFFERENCE BETWEEN MAX AND MIN IS : ",a-b)
